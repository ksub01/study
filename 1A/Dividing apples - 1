int main()
{
    int n;
    cin >> n;
    int k;
    cin >> k;
    cout << n/k;
}



Хорошие решения
#include <iostream>

using namespace std;

int main()
{
	int n, k;
	cin >> n >> k;
	
	cout << k / n << endl;

	return 0;
}
