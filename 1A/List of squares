a1 = int(input())
for i in range(1, int(a1**0.5)+1):
    print(i**2)
    
    
Найди ошибку: Список квадратов
#include <iostream>
using namespace std;
int main() {
	int N;
	cin >> N;
	for (int counter = 1; counter*counter < N; counter++) {
		cout << counter * counter << " ";
	}
	return 0;
}



b = int(input())
for i in range (1, b + 1):
   if b > i * i:
      print(i * i)
      

#include <iostream>
#include <cmath>

using namespace std;

int main() {
    int n;
    cin >> n;
    int i=1;
    int k=1;
    while (k<n) {
        cout << k << endl;
        i+=1;
        k=i*i;
    }
    return 0;
}


Хорошие решения
num = int(input())
i = 1
while i ** 2 <= num:
    print(i ** 2)
    i += 1
