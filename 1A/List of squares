#include <iostream>
using namespace std;


int main() {
	int n;
	cin >> n;
	int i = 1;
	while (i * i <= n) {
		cout << (i * i) << endl;
		++i;
	}
	return 0;
}
Кодировка: 
Скачать
var
  n, i: integer;

begin
  readln(n);
  for i := 1 to n do
    if i * i <= n then writeln(i * i);
end.
