b = int(input())
c = int(input())
print(c % b)


Найди ошибку: Дележ яблок - 2
var n, k, ans : integer;

begin
  read(n, k);
  ans := k div n;
  write(ans);
end.


Хорошие решения
#include <iostream>

using namespace std;

int main()
{
	int n, k;
	cin >> n >> k;

	cout << k % n << endl;

	return 0;
}
