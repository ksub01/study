b = int(input())
b %= 1440
print(b//60, b%60)


Найди ошибку: Электронные часы - 1
#!/usr/bin/env python3
# -*- coding: utf-8 -*-
"""
Created on Sat Aug 15 12:18:22 2020

@author: peteru
"""

m = int(input())

# how many time?
time = (m % 1440 )/ 60

# calculate residual minutes
minutes = (time % 1) * 60

# calculate hours
hours = time - (time % 1)

print(int(hours), int(minutes))

a = int(input())
b = a // 60
c = a % 60
print(b, c)


Хорошие решения
#include <iostream>

using namespace std;

int main()
{
	int a, h, m;
	cin >> a;
	cout << (a / 60) % 24 << " " << a % 60 << endl;
	return 0;
}
