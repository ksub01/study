a1 = int(input())
b1 = int(input())
c1 = int(input())
a2 = int(input())
b2 = int(input())
c2 = int(input())
c1 = a1 * 3600 + b1 * 60 + c1
c2 = a2 * 3600 + b2 * 60 + c2
print(c2 - c1)
