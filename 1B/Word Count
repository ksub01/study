k = input().split(' ')
print(len(k))


Хорошие решения
#include <iostream>
#include <cstring>
#include <cstdio>
using namespace std;




int main()
{
	string a;
	int k = 1;
    getline(cin, a);
	for (int i = 0; i < a.length(); i++)
	{
		if (a[i] == ' ')
		{
			k++;
		}
	}
	cout << k;
	return 0;
}
