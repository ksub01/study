list1 = []
n, m = map(int, input().split(' '))
for i in range(n):
    list1.append([])
    for k in range(m):
        list1[i].append(None)

for i in range(m):
    list1[0][i] = 1
for i in range(n):
    list1[i][0] = 1

for i in range(1, n):
    for k in range(1, m):
        list1[i][k] = list1[i-1][k] + list1[i][k-1]
for i in range(n):
    for k in range(m):
        list1[i][k] = str(list1[i][k])
for i in list1:
    print(' '.join(i))
