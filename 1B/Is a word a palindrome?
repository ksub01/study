var
  a: array [0..100, 0..100] of integer;
  n, i, j: integer;
  f: boolean;

begin
  readln(n);
  for i := 1 to n do 
    for j := 1 to n do 
      read(a[i, j]);
  f := true;
  for i := 1 to n do 
    for j := i + 1 to n do 
    begin
      if a[i, j] <> a[j, i] then 
        f := false;
    end;
  if f then
    writeln('yes')
  else
    writeln('no');
end.



