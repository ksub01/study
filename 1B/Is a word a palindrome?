# coding: utf-8

# In[12]:


n = int(input())
a = []
s = 0
z = 0
for i in range(n):
    a.append(list(map(int, input().split())))
for k in range (0,n):
    for i in range(k+1,n):
        if a[k][i]!=a[i][k]:
            z = 1
if (z==1):
    print('no')
else:
    print('yes')

