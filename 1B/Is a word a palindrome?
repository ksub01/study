{$mode delphi}
var a:array[0..99,0..99] of integer;
    i,j,n,b:integer;
begin
b:=0;
read(n);
for i:=0 to (n-1) do 
 for j:=0 to (n-1) do 
 read(a[i,j]);
for i:=0 to (n-1) do  
 for j:=0 to (n-1) do
  if a[i,j]<>a[j,i] then 
   b:=1;
if b=1 then 
 writeln('no') else
  writeln('yes');
end.

