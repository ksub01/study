a = float(input())
b = float(input())
c = float(input())
eps = 1e-7
if abs(a + b - c) < eps:
    print("YES")
else:
    print("NO")



Найди ошибку: a + b = c
import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class aplusb {
    public static void main(String[] args) throws IOException {
        final BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

        var eps = 1e-6;
        var first = Double.parseDouble(br.readLine());
        var second = Double.parseDouble(br.readLine());
        var third = Double.parseDouble(br.readLine());

        var result = first + second;

        if (Math.abs(result - third) < eps) {
            System.out.println("YES");
        } else {
            System.out.println("NO");
        }
    }
}
