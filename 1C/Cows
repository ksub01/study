n = int(input())
if n % 10 == 1 and n != 11:
    print(n, 'korova')
elif 11 <= n <= 14:
    print(n, 'korov')
elif 2 <= (n % 10) <= 4:
    print(n, 'korovy')
else:
    print(n, 'korov')


Найди ошибку: Коровы
a=int(input())
if a%10==0 or a%10>=5 or a==12 or a==13 or a==14:
    print(a,'korov')
elif a%10==2 or a%10==3 or a%10==4:
    print(a,'korovy')
elif a%10==1:
    print(a,'korova')
    
    
n = int(input())
if n == 1:
    print(n, "korova")
elif n == 2 or n == 3 or n == 4:
    print(n, "korovy")
elif n > 20 and n % 10 == 1:
    print(n, "korova")
elif n > 20 and n % 10 == 2 or n % 10 == 3 or n % 10 == 4:
    print(n, "korovy")
else:
    print(n, "korov")
    

