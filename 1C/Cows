n=int(input())
if (n%10==2 or n%10==3 or n%10==4) and (n//10)!=1:
    print(n,'korovy')
elif n%10==1 and (n//10)!=1:
    print(n,'korova')
else:
    print(n,'korov')

    
Кодировка: 
Скачать
#include <iostream>
#include <cmath>

using namespace std;

int main()
{
	int n;
	cin >> n;
	if (n % 10 == 1 && n != 11) {
		cout << n << " korova" << endl;
	}
	else if (n % 10 >= 5 || n / 10 == 1 || n % 10 == 0) {
		cout << n << " korov" << endl;
	}
	else {
		cout << n << " korovy" << endl;
	}
	return 0;
}

