Кодировка: 
Скачать
#include <iostream>

using namespace std;

int main()
{
	int x, y, x1, y1;
	cin >> x >> y >> x1 >> y1;
	if (x == x1 || y == y1)
	{
		cout << "YES" << endl;
	}
	else
	{
		cout << "NO" << endl;
	}
}
