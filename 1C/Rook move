#include <iostream>

using namespace std;

int main() {
  int i0, j0, i1, j1;
  cin >> i0 >> j0 >> i1 >> j1;
  if ( i0 == j0 || j0 == j1) {
    cout << "YES" << endl;
  } else {
    cout << "NO" << endl;
  }
}



Хорошие решения
x1 = int(input())
y1 = int(input())

x2 = int(input())
y2 = int(input())

if x1 == x2 or y1 == y2:
    print('YES')
else:
    print('NO')

