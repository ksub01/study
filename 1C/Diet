a, b, c = map(float, input().split(' '))
n = int(input())
x1 = x2 = x3 = 0
e = 1e-5
for i in range(n):
    a1, b1, c1, d1 = map(float, input().split(' '))
    x1 += a1*d1
    x2 += b1*d1
    x3 += c1*d1
if x1 > a-e and x2 > b-e and x3 > c-e:
    print('YES')
else:
    print('NO')
#print(x1, x2, x3)
