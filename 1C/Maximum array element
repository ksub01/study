a=list(map(int,input().split()))
e=0
m=1
for i in range(len(a)):
   if a[i]>e:
      e=a[i]
print(e)
   
   
   


Хорошие решения
#include <iostream>
#include <cmath>

using namespace std;

int main() {
	int n, max=-100500, a;
	
	cin >> n;
	
	for (int i=0; i<n; i++) {
		cin >> a;
		if (a>max) {
			max = a;
		}
	
	}
	cout << max;
	return 0;
}
