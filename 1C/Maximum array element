n = input()
list1 = list(map(int, input().split()))
l = -1001
for i in list1:
    l = max(l, i)
print(l)


Найди ошибку: Максимальный элемент массива
#include <iostream>
#include<string>
using namespace std;

int main()
{
	int n, max = 0, a;
	cin >> n;
	for (int i = 0; i < n; i++) {
		cin >> a;
		if (a > max) {
			max = a;
		}
	}
	cout << max;
	return 0;
}


from math import *
