#include <iostream>
#include <cmath>

using namespace std;

int main(){
    double a, b, c, eps = 1e-8;
    cin >> a >> b >> c;
    if (abs(a + b - c) < eps)
        cout << "YES";
    else
        cout << "NO";
}
